Fruit={handleDeleteFruit}\n        onUpdateFruit={handleUpdateFruit}\n      />\n    </main>\n  );\n}\n\nexport default App;\n",["36","37"],"/Users/shulin/Development/code/phase-3/phase-3-final-project/phase-3-sinatra-react-project-client/src/Search.js",[],[],"/Users/shulin/Development/code/phase-3/phase-3-final-project/phase-3-sinatra-react-project-client/src/FruitList.js",[],[],"/Users/shulin/Development/code/phase-3/phase-3-final-project/phase-3-sinatra-react-project-client/src/NewFruitForm.js",[],[],"/Users/shulin/Development/code/phase-3/phase-3-final-project/phase-3-sinatra-react-project-client/src/Header.js",[],[],{"ruleId":"38","severity":1,"message":"39","line":10,"column":10,"nodeType":"40","messageId":"41","endLine":10,"endColumn":23},{"ruleId":"38","severity":1,"message":"42","line":10,"column":25,"nodeType":"40","messageId":"41","endLine":10,"endColumn":41},{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","replacedBy":"46"},"no-unused-vars","'selectedOwner' is assigned a value but never used.","Identifier","unusedVar","'setSelectedOwner' is assigned a value but never used.","no-native-reassign",["47"],"no-negated-in-lhs",["48"],"no-global-assign","no-unsafe-negation"]