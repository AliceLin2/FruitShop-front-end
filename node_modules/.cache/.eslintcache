 {\n      if (fruit.id === updatedFruit.id) {\n        return updatedFruit;\n      } else {\n        return fruit;\n      }\n    });\n    setFruits(updatedFruits)\n    setFormData(empty)\n  }\n\n  const showFruits = fruits.filter((fruit) =>\n    fruit.name.toLowerCase().includes(search.toLowerCase())\n  );\n  \n  return (\n    <div style={{backgroundImage: `url(${background})`}}>\n      <Header \n        owners={owners} \n        onChangeSelectedOwner={setSelectedOwner} \n        selectedOwner={selectedOwner} \n        onChangeFormData={setFormData}\n        empty={empty}\n      />\n      <Search search={search} onSearch={setSearch}/>\n      {selectedOwner===\"All\"\n        ? \"\"\n        :\n        <FruitForm \n          onAddFruit={handleAddFruit} \n          onUpdateFruit={handleUpdateFruit} \n          formData={formData}\n          onChangeFormData={setFormData}\n        />}\n      <FruitList\n        fruits={showFruits}\n        onDeleteFruit={handleDeleteFruit}\n        onChangeFormData={setFormData}\n        onChangeSelectedOwner={setSelectedOwner}\n      />\n    </div>\n  );\n}\n\nexport default App;\n","/Users/shulin/Development/code/phase-3/phase-3-final-project/phase-3-sinatra-react-project-client/src/Search.js",[],[],"/Users/shulin/Development/code/phase-3/phase-3-final-project/phase-3-sinatra-react-project-client/src/FruitList.js",[],[],"/Users/shulin/Development/code/phase-3/phase-3-final-project/phase-3-sinatra-react-project-client/src/Header.js",[],[],"/Users/shulin/Development/code/phase-3/phase-3-final-project/phase-3-sinatra-react-project-client/src/FruitForm.js",[],[],{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","replacedBy":"40"},{"ruleId":"41","severity":1,"message":"42","line":29,"column":22,"nodeType":"43","messageId":"44","endLine":29,"endColumn":24},"no-native-reassign",["45"],"no-negated-in-lhs",["46"],"eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","no-global-assign","no-unsafe-negation"]